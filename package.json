{
  "name": "@kylegillen/nestjs-fastify-better-auth",
  "type": "module",
  "version": "2.0.2",
  "packageManager": "pnpm@10.18.3",
  "description": "Better Auth for NestJS and Fastify",
  "author": "Kyle Gillen",
  "license": "MIT",
  "homepage": "https://github.com/kylegillen/nestjs-fastify-better-auth",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylegillen/nestjs-fastify-better-auth.git"
  },
  "bugs": {
    "url": "https://github.com/kylegillen/nestjs-fastify-better-auth/issues"
  },
  "keywords": [
    "nestjs",
    "better-auth",
    "fastify",
    "auth",
    "nestjs-fastify-better-auth"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pnpm ci:publish",
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "vitest",
    "ci:publish": "pnpm clean && pnpm build && pnpm lint && pnpm check-exports && pnpm test",
    "local-release": "changeset version && changeset publish",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run --pass-with-no-tests"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-fastify": "^11.1.6",
    "better-auth": ">=1.3.8 <2.0.0",
    "fastify": "~5.4.0",
    "graphql": "^16.11.0",
    "socket.io": "^4.8.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@arethetypeswrong/cli": "^0.18.2",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.8.1",
    "@vitest/coverage-v8": "^3.2.4",
    "better-auth": "^1.3.28",
    "eslint": "^9.38.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.8",
    "vitest": "^3.2.4"
  }
}
